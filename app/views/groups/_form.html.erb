<%= form_for group do |f| %>
  <div class="chat-group-form__errors">
    <h2 class ="hh2">10error</h2>
    <ul class ="uul">
      <li class ="lli">namewonyuuryoku</li>
    </ul>
  </div>
  <div class="chat-group-form__field">
    <div class="chat-group-form__field--left">
      <%= f.label :name, class: 'chat-group-form__label' %>
    </div>
    <div class="chat-group-form__field--right">
      <%= f.text_field :name, class: 'chat__group_name chat-group-form__input', placeholder: 'groupmeiwo' %>
    </div>
  </div>
  <div class="chat-group-form__field clearfix"></div>
  <div class="chat-group-form__field clearfix">
    <div class="chat-group-form__field--left">
      <label class="chat-group-form__label" for="chat_group_chat-member">chat-member</label>
    </div>
    <div class="chat-group-form__field--right">
      <%= f.collection_check_boxes :user_ids, User.all, :id, :name %>
    </div>
  </div>
  <div class="chat-group-form__field clearfix">
    <div class="chat-group-form__field--left"></div>
    <div class="chat-group-form__field--right">
      <%= f.submit class: 'chat-group-form__action-btn' %>
    </div>
  </div>
<% end %>
